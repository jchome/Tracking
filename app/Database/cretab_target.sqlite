CREATE TABLE track_target (
	id integer NOT NULL PRIMARY KEY AUTOINCREMENT , 
	app_id integer NOT NULL , 
	name varchar(4000) NOT NULL , 
	code varchar(4000) NOT NULL , 
	selector varchar(4000) 
);



